                <TextBlock Text="{Binding Ticket_ID}"
                           Grid.Column="1"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Pack_No}"
