                    Click="btnEmptyBox_Click" />
        </DataTemplate>

        <DataTemplate x:Key="LotteryBoxDataTemplate">
            <Button x:Name="btnDashboard"
                    Content="{Binding Box_No,Mode=TwoWay}"
                    Style="{StaticResource BoxTicketButtonStyle}"
                    Click="btnDashboard_Click"/>
        </DataTemplate>

        <DataTemplate x:Key="ActivatedTicketDataTemplate">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="90" />
                    <ColumnDefinition Width="310" />
                </Grid.ColumnDefinitions>
                <Border Margin="10,0,10,0"
                        Height="40"
                        Width="70"
                        BorderThickness="2"
                        BorderBrush="{StaticResource GrayButtonMouseoverBackgroundColor}"
                        VerticalAlignment="Top">
                    <TextBlock x:Name="txtBoxNo"
                               Text="{Binding Box_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                               Height="30"
                               Width="70"
                               FontSize="20"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Center"
                               TextAlignment="Center" />
                </Border>
                <controls:Expander  Header="{Binding Ticket_Name}"
                                    Width="300"
                                    IsExpanded="False"
                                    Grid.Column="1">
                    <Grid Background="{StaticResource WhiteColor}"
                          Height="250"
                          Width="300">
                        <Grid Margin="20,5,20,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100" />
                                <ColumnDefinition Width="2" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <TextBlock Text="Game Id"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBox x:Name="txtGameId_Display"
                                     Text="{Binding Game_Id, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Height="25"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <TextBlock Text="Pack Id"
                                       Grid.Row="1"
                                       HorizontalAlignment="Stretch"
                                       VerticalAlignment="Center" />
                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       Grid.Row="1"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />
                            <TextBox Text="{Binding Packet_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Grid.Row="1"
                                     Height="25"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <TextBlock Text="Ticket Name"
                                       Grid.Row="2"
                                       HorizontalAlignment="Stretch"
                                       VerticalAlignment="Center" />
                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       Grid.Row="2"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />
                            <TextBox Text="{Binding Ticket_Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Grid.Row="2"
                                     Height="25"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <TextBlock Text="Price"
                                       Grid.Row="3"
                                       HorizontalAlignment="Stretch"
                                       VerticalAlignment="Center" />
                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       Grid.Row="3"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />
                            <TextBox Text="{Binding Price, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Grid.Row="3"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <TextBlock Text="Start No."
                                       Grid.Row="4"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       Grid.Row="4"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBox x:Name="txtStartNo_Display"
                                     Text="{Binding Start_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Grid.Row="4"
                                     Height="25"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <TextBlock Text="End No."
                                       Grid.Row="5"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBlock Text=":"
                                       Grid.Column="1"
                                       Grid.Row="5"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center" />

                            <TextBox x:Name="txtEndNo_Display"
                                     Text="{Binding End_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                     Grid.Column="2"
                                     Grid.Row="5"
                                     Height="25"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center"
                                     Margin="10,0" />

                            <!--<Button x:Name="btnEditTicket" Content="Edit" Click="btnEditTicket_Click"  Grid.Column="2" Grid.Row="4" Style="{StaticResource TextButtonStyle}"/>-->
                        </Grid>
                    </Grid>
                </controls:Expander>
            </Grid>
        </DataTemplate>

        <DataTemplate x:Key="CloseShiftDataTemplate">
            <Button x:Name="btnCloseShiftDashboard"
                    Content="{Binding Box_No,Mode=TwoWay}"
                    Click="btnCloseShiftDashboard_Click">
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="HorizontalAlignment"
                                Value="Stretch" />
                        <Setter Property="VerticalAlignment"
                                Value="Stretch" />
                        <Setter Property="Height"
                                Value="110" />
                        <Setter Property="Width"
                                Value="110" />
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="Button">

                                    <Grid Height="110"
                                          Width="110"
                                          Margin="0">
                                        <Border x:Name="bd"
                                                Height="110"
                                                Width="110 "
                                                BorderThickness="2">
                                            <Border  CornerRadius="5"
                                                     Margin="7"
                                                     Background="{Binding Status, Converter={StaticResource CallStatusBackgroundConverter}}">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="18" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="20" />
                                                    </Grid.RowDefinitions>

                                                    <StackPanel Orientation="Horizontal"
                                                                Margin="5,0,0,0">
                                                        <TextBlock Text="$"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                        <TextBlock Text="{Binding Price,Mode=TwoWay}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                    </StackPanel>

                                                    <TextBlock Text="{Binding Box_No,Mode=TwoWay}"
                                                               Grid.Row="1"
                                                               Foreground="{StaticResource WhiteColor}"
                                                               FontSize="35"
                                                               FontWeight="Bold"
                                                               HorizontalAlignment="Center"
                                                               VerticalAlignment="Center" />
                                                    <StackPanel Orientation="Horizontal"
                                                                Grid.Row="2"
                                                                Margin="5,0,0,0">
                                                        <TextBlock Text="o"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                        <TextBlock Text=":"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                        <TextBlock Text="{Binding Start_No,Mode=TwoWay}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal"
                                                                Margin="0,0,5,0"
                                                                HorizontalAlignment="Right">
                                                        <TextBlock Text="$"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   Visibility="{Binding Status, Converter={StaticResource CloseBoxVisibility}}"
                                                                   FontWeight="SemiBold" />

                                                        <TextBlock Text="{Binding Total_Price,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   Visibility="{Binding Status, Converter={StaticResource CloseBoxVisibility}}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />
                                                    </StackPanel>
                                                    <TextBlock Text="S"
                                                               Grid.Row="2"
                                                               Visibility="{Binding Status, Converter={StaticResource TextVisibilityConvertor}}"
                                                               Foreground="{StaticResource WhiteColor}"
                                                               FontSize="14"
                                                               FontWeight="SemiBold"
                                                               HorizontalAlignment="Right"
                                                               Margin="0,0,5,0" />
                                                    <StackPanel Orientation="Horizontal"
                                                                Grid.Row="2"
                                                                HorizontalAlignment="Right"
                                                                Margin="0,0,5,0">

                                                        <TextBlock Text="S"
                                                                   Visibility="{Binding Status, Converter={StaticResource TextVisibilityConverter}}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />

                                                        <TextBlock Text="c"
                                                                   Visibility="{Binding Status, Converter={StaticResource CloseBoxVisibility}}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />

                                                        <TextBlock Text=":"
                                                                   Visibility="{Binding Status, Converter={StaticResource CloseBoxVisibility}}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />

                                                        <TextBlock Text="{Binding Stopped_At,Mode=TwoWay}"
                                                                   Visibility="{Binding Status, Converter={StaticResource CloseBoxVisibility}}"
                                                                   Foreground="{StaticResource WhiteColor}"
                                                                   FontSize="14"
                                                                   FontWeight="SemiBold" />

                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Border>
                                        <VisualStateManager.VisualStateGroups>
                                            <VisualStateGroup x:Name="CommonStates">
                                                <VisualState x:Name="Normal" />

                                                <VisualState x:Name="PointerOver">
                                                    <Storyboard>
                                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="BorderBrush"
                                                                                       Storyboard.TargetName="bd">
                                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                                    Value="Gray" />
                                                        </ObjectAnimationUsingKeyFrames>
                                                    </Storyboard>
                                                </VisualState>

                                                <VisualState x:Name="Pressed">
                                                    <Storyboard>
                                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="BorderBrush"
                                                                                       Storyboard.TargetName="bd">
                                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                                    Value="{StaticResource ButtonBackgroundLightOrange}" />
                                                        </ObjectAnimationUsingKeyFrames>
                                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Height"
                                                                                       Storyboard.TargetName="bd">
                                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                                    Value="100" />
                                                        </ObjectAnimationUsingKeyFrames>
