                                                 IsReadOnly="True" />
                                        <TextBox x:Name="txtNewstoreActiveStartNo"
                                                 TextWrapping="Wrap"
                                                 Text="{Binding Active_StatusObj.Start_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                 Grid.Column="1"
                                                 Margin="0,0,20,0"
                                                 FontSize="12"
                                                 MinHeight="30"
                                                 Height="30"
                                                 Grid.Row="4" />
                                        <TextBox x:Name="txtNewstoreActiveEndNo"
                                                 TextWrapping="Wrap"
                                                 Text="{Binding Active_StatusObj.End_No, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                 Grid.Column="1"
                                                 Margin="0,0,20,0"
                                                 FontSize="12"
                                                 MinHeight="30"
                                                 Height="30"
                                                 Grid.Row="5"
                                                 IsReadOnly="True" />
                                        <StackPanel Orientation="Horizontal"
                                                    HorizontalAlignment="Right"
                                                    VerticalAlignment="Center"
                                                    Grid.Column="1"
                                                    Margin="0,0,20,0"
                                                    Grid.Row="7">

                                            <Button x:Name="BtnSkip"
                                                    Content="Skip"
                                                    Command="{Binding Next}"
                                                    Style="{StaticResource TextButtonStyle}"
                                                    Margin="10,0,0,0" />

                                            <Button x:Name="btnRemove"
                                                    Content="Remove"
                                                    Command="{Binding RemoveboxCommand,Mode=TwoWay}"
                                                    Style="{StaticResource TextButtonStyle}"
                                                    Margin="10,0,0,0"
                                                    VerticalAlignment="Stretch" />



                                            <Button x:Name="btnFinish"
                                                    Content="Finish"
                                                    Style="{StaticResource TextButtonStyle}"
                                                    Command="{Binding FinishSetupCommand,Mode=TwoWay}"
                                                    Margin="10,0,0,0" />
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Border>
