                <TextBlock Text="{Binding Ticket_Name}"
                           Grid.Column="3"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Open_No}"
                           Grid.Column="4"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Close_No}"
                           Grid.Column="5"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Ticket_Count}"
                           Grid.Column="6"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Ticket_Value}"
                           Grid.Column="7"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
                <TextBlock Text="{Binding Total}"
                           Grid.Column="8"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           FontSize="13" />
