<Page x:Class="Lottery_Application.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:Lottery_Application"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:AttachedProperties="using:Lottery_Application.AttachedProperties"
      xmlns:converter="using:Lottery_Application.Converters"
      xmlns:controls="using:Microsoft.Toolkit.Uwp.UI.Controls"
      xmlns:extended="using:MyToolkit.Controls"
      xmlns:userControl="using:Lottery_Application.Usercontrol"
      xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf"
      mc:Ignorable="d">
    <Page.Resources>
        <converter:CallStatusEnumToBackgroundColor x:Key="CallStatusBackgroundConverter" />
        <converter:BooleanToVisibilityConverter x:Key="BoolToVisibility" />
        <converter:DateFormatConverter x:Key="DateFormatConverter" />
        <converter:DataToVisibilityConverter x:Key="TextVisibilityConvertor" />
        <converter:StringToTimesapn x:Key="TimeshowConvertor" />
        
        <converter:SettleToVisibility x:Key="SettleNotsettle" />
        <converter:AstericSignVisibility x:Key="AstericSignVisibilityConverter" />
        <converter:CloseBoxVisibilityConverter x:Key="CloseBoxVisibility" />

        <DataTemplate x:Key="ShiftSellSummaryTemplate">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="30" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel Grid.Row="0"
                            Orientation="Horizontal">
                    <TextBlock Text="Shift" />
                    <TextBlock Text="{Binding ShiftID,Mode=TwoWay}"
                               Margin="5,0,0,0" />

                </StackPanel>
                <StackPanel Grid.Row="1">
                    <TextBlock Text="{Binding ScratchSells,Mode=TwoWay}" />
                    <TextBlock Text="{Binding OnlineSells,Mode=TwoWay}"
                               Margin="0,5,0,0" />
                    <TextBlock Text="{Binding TotalSells,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                               Margin="0,5,0,0" />
                    <TextBlock Text="{Binding ScratchPayout,Mode=TwoWay}"
                               Margin="0,5,0,0" />
                    <TextBlock Text="{Binding OnlinePayout,Mode=TwoWay}"
                               Margin="0,5,0,0" />
                    <TextBlock Text="{Binding TotalPayout,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                               Margin="0,5,0,0" />
                    <TextBlock Text="{Binding Loan,Mode=TwoWay}"
                               Margin="0,5,0,0" />
                </StackPanel>
            </Grid>
        </DataTemplate>

        <Style x:Key="PrevoiusButtonStyle"
               TargetType="Button">
            <Setter Property="HorizontalAlignment"
                    Value="Stretch" />
            <Setter Property="VerticalAlignment"
                    Value="Stretch" />
            <Setter Property="Height"
                    Value="40" />
            <Setter Property="Width"
                    Value="60" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border x:Name="border"
                                Background="Transparent">
                            <Path x:Name="Prevoius_Buton"
                                  VerticalAlignment="Bottom"
                                  Data="M12.970992,0L21.524992,0 11.658996,12.152008 8.5930022,15.931 8.6660002,16.020996 11.772003,19.846008 21.636,32 13.082,32 4.3889922,21.290009 4.3150023,21.200989 4.2769927,21.247009 0,15.977997 4.2769927,10.708008z"
                                  Height="20"
                                  Width="20"
                                  Stretch="Uniform"
                                  Fill="{StaticResource EmptyBoxBackgroundColor}" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>

            </Setter>
        </Style>

        <Style x:Key="ReportButtonStyle" TargetType="ToggleButton">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate x:Name="ReportTemplate" TargetType="ToggleButton">
                        <Grid>
                        <Border x:Name="OuterBd"
                                BorderBrush="SkyBlue"
                                Background="SkyBlue"
                                BorderThickness="3"
                                CornerRadius="10" Height="30" Width="60">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                    <Border CornerRadius="50"
                                            BorderBrush="White"
                                            BorderThickness="1"
                                            Background="White"
                                            Height="20"
                                            Width="20">
                                        <Button x:Name="Circle1"
                                                Background="White"
                                                Height="13"
                                                Width="13"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" />
                                    </Border>

                                    <Border Grid.Column="1"
                                            CornerRadius="50"
                                            BorderBrush="White"
                                            BorderThickness="1"
                                            Background="White"
                                            Height="20"
                                            Width="20">
                                        <Button x:Name="Circle2"
                                                Background="White"
                                                Height="13"
                                                Width="13"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"/>
                                    </Border>
                                
                            </Grid>
                        </Border>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Checked">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Circle1"
                                                                           Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="Visible" />
                                            </ObjectAnimationUsingKeyFrames>

                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Circle2"
                                                                           Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="Collapsed" />
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                </VisualState>

                                    <VisualState x:Name="PointerOver">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Background"
                                                                           Storyboard.TargetName="Circle1">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="White" />
                                            </ObjectAnimationUsingKeyFrames>

                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Background"
                                                                           Storyboard.TargetName="Circle2">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="White" />
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="UnChecked">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Circle2"
                                                                           Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="Visible" />
                                            </ObjectAnimationUsingKeyFrames>

                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Circle1"
                                                                           Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0"
                                                                        Value="Collapsed" />
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        
        <Style x:Key="NextButtonStyle"
               TargetType="Button">
            <Setter Property="HorizontalAlignment"
                    Value="Stretch" />
            <Setter Property="VerticalAlignment"
                    Value="Stretch" />
            <Setter Property="Height"
                    Value="40" />
            <Setter Property="Width"
                    Value="60" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border x:Name="border"
                                Background="Transparent">
                            <Path x:Name="Next_Button"
                                  VerticalAlignment="Bottom"
                                  Data="M0,0L8.5539884,0 17.246985,10.709991 17.32099,10.799011 17.359992,10.752991 21.637002,16.022003 17.359992,21.291992 8.6659879,32 0.11099243,32 9.9780006,19.847992 13.043004,16.069 12.970006,15.977997 9.8639869,12.153992z"
                                  Height="20"
                                  Width="20"
                                  Stretch="Uniform"
                                  Fill="{StaticResource EmptyBoxBackgroundColor}" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>

            </Setter>


        </Style>

